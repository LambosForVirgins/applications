{
  "name": "LFV Rewards",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Your reward wingman",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://api.devnet.solana.com/"
  ]
}